  "scripts": {
    "start": "env-cmd -f .env.development.local react-scripts start",
    "build": "env-cmd -f .env.development react-scripts build",
    "build:qa": "env-cmd -f .env.qa react-scripts build",
    "build:stg": "env-cmd -f .env.stg react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --maxWorkers=4",
    "test:watch": "react-scripts test --coverage --watchAll=true --maxWorkers=4",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/features/walletUser/state/*.{js,jsx,ts,tsx}",
      "!src/features/walletUser/model/*.{js,jsx,ts,tsx}",
      "!src/features/walletUser/enums/*.{js,jsx,ts,tsx}",
      "!src/models/*.{js,jsx,ts,tsx}",
      "!src/react-app-env.d.ts"
    ]
  },
  "devDependencies": {
